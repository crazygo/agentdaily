n            background-color: #fff4e6;\n            color: #cc6600;\n        }\n\n        .community-quote {\n            background-color: #fafafa;\n            border-left: 3px solid #ff6600;\n            padding: 6px 8px;\n            margin-top: 6px;\n            font-size: 12px;\n            font-style: italic;\n            color: #444;\n        }\n\n        .community-quote .quote-source {\n            display: block;\n            font-size: 10px;\n            color: #828282;\n            margin-top: 3px;\n            font-style: normal;\n        }\n\n        .source-list {\n            font-size: 11px;\n            color: #828282;\n            margin-top: 4px;\n        }\n\n        .source-list span {\n            display: inline-block;\n            background-color: #f5f5f5;\n            padding: 2px 5px;\n            margin: 2px 2px 0 0;\n            border-radius: 2px;\n        }\n\n        .topics {\n            margin-top: 6px;\n        }\n\n        .topic-tag {\n            display: inline-block;\n            background-color: #fff8e6;\n            color: #996600;\n            padding: 2px 6px;\n            border-radius: 3px;\n            font-size: 10px;\n            margin: 2px 2px 0 0;\n        }\n\n        .insight-author {\n            font-size: 12px;\n            color: #828282;\n            margin-bottom: 4px;\n        }\n\n        .insight-source {\n            font-size: 11px;\n            color: #ff6600;\n            margin-bottom: 4px;\n        }\n\n        footer {\n            background-color: #f6f6ef;\n            border-top: 1px solid #d4d4d4;\n            padding: 12px;\n            text-align: center;\n            font-size: 12px;\n            color: #828282;\n            margin-top: 20px;\n        }\n\n        footer a {\n            color: #ff6600;\n            text-decoration: none;\n        }\n\n        footer a:hover {\n            text-decoration: underline;\n        }\n\n        .empty-state {\n            color: #828282;\n  